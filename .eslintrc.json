{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "autofix"
    ],
    "rules": {
        "linebreak-style":"off",
        "no-plusplus":"off",
        "autofix/radix": "error",
        "max-len":"off",
        "no-empty":"warn"
    }
}
